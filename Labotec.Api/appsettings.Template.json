{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "ConnectionStrings": {
    "Default": "Server=localhost;Port=3306;Database=labotec;Uid=root;Pwd=localpassword;SslMode=Preferred;"
  },
  "Jwt": {
    "Issuer": "Labotec.Api",
    "Audience": "Labotec.Client",
    "Key": "REPLACE_WITH_A_STRONG_SECRET_KEY"
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "http://localhost:8080"
    ]
  },
  "Storage": {
    "Provider": "File",
    "File": {
      "BasePath": "wwwroot/uploads",
      "PublicBaseUrl": ""
    },
    "Azure": {
      "ConnectionString": "",
      "Container": "labresults",
      "CdnUrl": ""
    }
  }
}
